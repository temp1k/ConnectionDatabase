                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Label Content="РЕГИСТРАЦИЯ АДМИНИСТРАТОРА" Grid.Row="1" Grid.Column="1" FontSize="25" Foreground="OrangeRed" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center"/>
